{
  "expo": {
    "name": "자재로",
    "slug": "expo-zazero-app",
    "sdkVersion": "46.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/zazero_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "svg"
      ]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.koreajason.expozazeroapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/zazero_icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.koreajason.expozazeroapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "960980ea-d25e-4228-a133-be5e8d69d34c"
      }
    }
  }
}
