{
  "expo": {
    "name": "자재로",
    "slug": "zazaero",
    "version": "2.0.0",
    "sdkVersion": "46.0.0",
    "orientation": "portrait",
    "icon": "./assets/zazero_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/zazero_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#3D40E0"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "svg"
      ]
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.www.zazaero",
      "googleServicesFile": "./GoogleService-Info.plist",
      "buildNumber": "10"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/zazero_icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.www.zazaero",
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "versionCode": 10
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "df26d83b-0020-45b2-846c-2a545d26af57"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/df26d83b-0020-45b2-846c-2a545d26af57"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "13.0"
          }
        }

      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
