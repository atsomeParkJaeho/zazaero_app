{
  "expo": {
    "name": "자재로",
    "slug": "zazaero",
    "version": "4.0.0",
    "sdkVersion": "46.0.0",
    "orientation": "portrait",
    "icon": "./assets/zazero_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/zazero_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#3D40E0"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "svg"
      ]
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.zazaero.www",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "kftc-bankpay",
          "ispmobile",
          "itms-apps",
          "hdcardappcardansimclick",
          "smhyundaiansimclick",
          "shinhan-sr-ansimclick",
          "smshinhanansimclick",
          "kb-acp",
          "mpocket.online.ansimclick",
          "ansimclickscard",
          "ansimclickipcollect",
          "vguardstart",
          "samsungpay",
          "scardcertiapp",
          "lottesmartpay",
          "lotteappcard",
          "cloudpay",
          "nhappcardansimclick",
          "nonghyupcardansimclick",
          "citispay",
          "citicardappkr",
          "citimobileapp",
          "kakaotalk",
          "payco",
          "chaipayment",
          "kb-auth",
          "hyundaicardappcardid",
          "com.wooricard.wcard",
          "lmslpay",
          "lguthepay-xpay",
          "liivbank",
          "supertoss"
        ],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSAllowsArbitraryLoadsInWebContent": true
        },
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ]
      },

      "buildNumber": "21"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/zazero_icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.www.zazaero",
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "versionCode": 18
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "df26d83b-0020-45b2-846c-2a545d26af57"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/df26d83b-0020-45b2-846c-2a545d26af57"
    },
    "plugins": [
      [
        "@notifee/react-native"
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "13.0",
            "useFrameworks": "static"
          }
        }
      ],
      [
        "@react-native-firebase/app"
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "$(PRODUCT_NAME)이(가) 사용자의 사진에 접근허용 합니다."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "$(PRODUCT_NAME)이(가) 사용자의 카메라에 사진을 허용합니다."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "$(PRODUCT_NAME)이(가) 사용자의 사진에 접근허용 합니다.",
          "savePhotosPermission": "$(PRODUCT_NAME)이(가) 사용자의 앨범에 저장을 허용합니다.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
